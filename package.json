    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "compression": "^1.7.4",
    "crossenv": "^0.0.2-security",
    "cssnano": "^6.0.1",
    "express": "^4.18.2",
    "happy-dom": "^12.9.1",
    "postcss": "^8.4.31",
    "sass": "^1.69.0",
    "serve-static": "^1.15.0",
